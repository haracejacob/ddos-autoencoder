net: "prototxt/ddos_ae.prototxt"
type : "Nesterov"
test_iter: 1
test_interval: 10000
test_compute_loss: true
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
stepsize: 8000
display: 1000
max_iter: 60000
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "prototxt/ddos_ae_nesterov"
momentum: 0.95
# solver mode: CPU or GPU
solver_mode: GPU