net: "prototxt/ddos_ae.prototxt"
type : "AdaDelta"
test_iter: 1
test_interval: 10000
test_compute_loss: true
base_lr: 1.0
lr_policy: "fixed"
delta: 1e-8
display: 1000
max_iter: 100000
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "prototxt/ddos_ae_delta"
momentum: 0.95
# solver mode: CPU or GPU
solver_mode: GPU